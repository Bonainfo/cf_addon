"                  margin:12px;\n"
"                  margin-top: 25px;\n"
"                  text-align:center;\n"
"                  word-wrap: break-word;\n"
"                  padding:7px;\n"
"                  word-wrap: normal;\n"
"                  }\n"
"                  #msg-container {\n"
"                  margin:12px;\n"
"                  }\n"
"                  #panel-1 {\n"
"                  font-family:Arial, 'Helvetica Neue', Helvetica, sans-serif;\n"
"                  font-weight:bold;\n"
"                  font-size:19px;\n"
"                  color:#378ED8;\n"
"                  margin-top:20px;\n"
"                  text-align:center;\n"
"                  word-wrap: normal;\n"
"                  }\n"
"                  #panel-2 {\n"
"                  font-family:Arial, 'Helvetica Neue', Helvetica, sans-serif;\n"
"                  font-weight:bold;\n"
"                  font-size:20px;\n"
"                  color:#777777;\n"
"                  margin-top:12px;\n"
"                  margin-bottom:5px;\n"
"                  text-align:center;\n"
"                  width:100%;\n"
"                  }\n"
"                  #product-img {\n"
"                  height:113px;\n"
"                  width:113px;\n"
"                  border-radius: 50%;\n"
