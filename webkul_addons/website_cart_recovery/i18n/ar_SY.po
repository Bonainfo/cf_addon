"                      <table id=\"t01\" width=\"100%\">\n"
"                        <thead style=\"border-bottom:2px solid #3F9ACF;\">\n"
"                          <tr>\n"
"                            <th>Product Image</th>\n"
"                            <th>Product Name</th>\n"
"                            <th>Qty.</th>\n"
"                            <th>Price/Unit</th>\n"
"                            <th>Total Price</th>\n"
"                          </tr>\n"
"                        </thead>\n"
"                      </table>\n"
"                    % endif\n"
"\n"
