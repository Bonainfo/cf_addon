"                  float:left;\n"
"                  border: 1px solid #CCCCCC;\n"
"                  margin:10px;\n"
"                  padding:0px;\n"
"                  margin-left: 0px;\n"
"                  }\n"
"                  .buy-now {\n"
"                  width:100%;\n"
"                  height:50px;\n"
"                  background-color:#53C5C4;\n"
